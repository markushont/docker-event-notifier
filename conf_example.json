{
    "settings": {
        "logging": "info",
        "tags": [
            "seh-green-01"
        ],
        "mode": "opt-out",
        "names": [
            "docker-events"
        ]
    },
    "events": {
        "container": {
            "die": {
                "attributes": {
                    "exitCode": "1"
                },
                "severity": "danger"
            },
            "stop": {
                "severity": "warning"
            },
            "start": {
                "severity": "good"
            }
        },
        "image": {
            "delete": {
                "severity": "warning"
            }
        }
    }
}